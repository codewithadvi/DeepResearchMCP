# Local Ollama Version - Environment Variables
# Copy this to .env and fill in your actual keys

# Required: LinkUp API Key (get from https://linkup.so)
LINKUP_API_KEY=your_linkup_api_key_here

# Optional: Groq API Key (fallback, get from https://console.groq.com)
# Only needed if Ollama isn't running
GROQ_API_KEY=your_groq_api_key_here

# Optional: Disable Telemetry
OTEL_SDK_DISABLED=true
